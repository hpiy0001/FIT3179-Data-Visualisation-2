{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 600,
  "height": 400,
  "data": {"url": "qualjob.csv"},
  "mark": "bar",
  "encoding": {
    "y": {
      "field": "Count",
      "type": "quantitative",
      "title": "Number of People (thousands)",
      "axis": {"format": ".0f"}
    },
    "x": {
      "field": "Occupation",
      "type": "nominal",
      "title": "Occupation",
      "axis": {
        "labelAngle": -45,
        "labelLimit": 200
      }
    },
    "color": {
      "field": "Qualification",
      "type": "nominal",
      "title": "Education Level",
      "scale": {
        "domain": [
          "Postgraduate Degree",
          "Graduate Diploma / Graduate Certificate",
          "Bachelor Degree",
          "Advanced Diploma/Diploma",
          "Certificate III/IV",
          "Certificate I/II",
          "Certificate n.f.d.",
          "Without non-school qualification"
        ],
        "range": ["#1f77b4", "#ff7f0e", "#2ca02c", "#d62728", "#9467bd", "#8c564b", "#e377c2", "#bcbd22"]
      }
    },
    "tooltip": [
      {"field": "Occupation", "title": "Occupation"},
      {"field": "Qualification", "title": "Education Level"},
      {"field": "Count", "title": "Count (thousands)", "format": ".1f"}
    ]
  }
}