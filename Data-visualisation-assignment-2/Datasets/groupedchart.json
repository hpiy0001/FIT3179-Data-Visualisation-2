{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 800,
  "height": 500,
  "data": {"url": "industryqualification.csv"},
  "mark": "bar",
  "encoding": {
    "x": {
      "field": "Industry",
      "type": "nominal",
      "title": "Industry",
      "axis": {
        "labelAngle": -45,
        "labelLimit": 200
      }
    },
    "y": {
      "field": "Count",
      "type": "quantitative",
      "title": "Average Weekly Earnings ($)",
      "axis": {"format": "$,.0f"}
    },
    "color": {
      "field": "Qualification",
      "type": "nominal",
      "title": "Education Level"
    },
    "xOffset": {
      "field": "Qualification"
    },
    "tooltip": [
      {"field": "Industry", "title": "Industry"},
      {"field": "Qualification", "title": "Education Level"},
      {"field": "Count", "title": "Weekly Earnings", "format": "$,.2f"}
    ]
  }
}